{
  "name": "zploited/laravel-identity",
  "description": "Identity client for Laravel, simplifying the authentication flows towards your Identity tenant.",
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Nielsen",
      "email": "dni@zploited.dk",
      "homepage": "https://www.zploited.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zploited\\Identity\\Client\\Laravel\\": "./src"
    }
